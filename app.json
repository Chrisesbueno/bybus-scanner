{
  "expo": {
    "name": "client",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/utils/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/utils/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFCE48"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.bybus",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./src/utils/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/utils/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "1a4c7d5d-2e72-4b44-b2d0-d00178789da8"
      }
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "css"
      ]
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Espero no tengas nudes"
        }
      ],
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ], 
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ]
  }
}